package java0425_class.part04;

/*
 * [출력결과]
 * 메뉴명:떡볶이
 * 가격:2000
 * 갯수:3
 * 메뉴금액:6000
 * 
 * 
 * 
 * 
 * 
 * 
 */
public class Java068_class {

	public static void main(String[] args) {
		Menushop od1 = new Menushop();
		od1.menu = "떡볶이";
		od1.price = 2000;
		od1.cnt = 3;
		
		od1.prn();
		
		

	}// end main()

}// end class
