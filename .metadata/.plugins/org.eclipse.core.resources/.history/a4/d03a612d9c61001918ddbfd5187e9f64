package java0418_basic;

public class Java012_operator {

	public static void main(String[] args) {
		int a = 3;
		int b = 5;
		int c = 7;
		
		// &&연산자의 좌변이 false이면 우변은 수행하지 않는다.
		boolean res = (++a > b) && (++b < c);
		System.out.println("a=" + a); //a=4
		System.out.println("b=" + b); //b=5
		System.out.println("res=" + res); //false
		

	}

}
