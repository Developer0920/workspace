package java0501_abstract_interface;

public class TruckAbs extends CarAbs{
	//빨간줄 난 부분을 클릭하면 자동 오버라이드 생성됨
	
	@Override
	public void work() {
		System.out.println("트럭에 짐을 싣고 있다.");
		
	} //추상클래스를 상속받은 클래스는.. 추상메소드롤 오버라이드 안 하면 이것도 그냥 추상클래스가 된다. 따라서 앞에 abstract를 명시해야 함.
		
	/*
	@Override
	public void work() { //이제 이 클래스는 추상클래스가 아니다.
		
		
	}
	*/
	
	
}

//근데 이걸 왜 쓰지?
