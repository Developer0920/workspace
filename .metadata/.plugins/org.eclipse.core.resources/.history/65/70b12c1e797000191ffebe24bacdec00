package java0507_exeption;
//다중 catch 사용법
public class Java151_exeption {

	public static void main(String[] args) {
		String data1 = "ab"; // ..
		String data2 = "0"; //분모는 0이 될수 없다. 상속관계가 아니므로 순서 상관 X
		
		try {
		int x = Integer.parseInt(data1);
		int y = Integer.parseInt(data2);
		int res = x/y;
		System.out.println(res);
		} catch 
		

	} //end main()

} //end class
