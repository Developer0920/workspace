package java0426_class.part02;
//클래스를 가지고 배열을 선언한다.

/*
 * [실행결과]
 * 상품		가격		상품재고		팔린수량
 * 
 * 
 */

public class Java074_class {

	public static void main(String[] args) {
		System.out.printf("%-14s %8s %5s %5s\n", "상품", "가격", "상품재고", "팔린수량");
		Goods nikon = new Goods("Nikon", 400000, 30, 50);
		Goods sony = new Goods("Sony", 450000, 20, 35);
		Goods fuji = new Goods("FujiFilm", 350000, 10, 20);
		nikon.prn();	
		sony.prn();
		fuji.prn();

	}// end main()

}// end class
