package java0419_statement;
/*
 * 1. data변수에 저장된 값이 영문자 대문자이면 "?는 대문자입니다",
 * 		소문자이면 "?는 소문자입니다."
 * 		그외는 "?는 기타입니다"로 출력하는 프로그램을 구현하시오.
 * 2. [출력결과]
 * 		data='D' => D는 대문자입니다.
 * 		data='d' => d는 소문자입니다.
 * 		data='1' => 1은 기타입니다.
 */
public class Java021_if {

	public static void main(String[] args) {
		char date = 'd';
		// 'A' <= data <= 'Z' --- 수학에서 쓰임
		// 'A' => 유니코드 65
		// 'a' => 유니코드 97
		// '0' => 유니코드 48
		System.out.println((int)'A');
		
		//if(data>='A' && data <= 'Z') { //문자는 연산자와 사용되면, 유니코드로 바꿔서 처리
			
		}
	}// end main
}// end class
