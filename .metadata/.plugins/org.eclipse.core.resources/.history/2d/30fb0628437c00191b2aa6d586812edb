package java0522_collection;

import java.util.HashMap;

/*
 * HashMap
 * 1 Map인터페이스를 구현한 클래스
 * 2 Hashtable은 동기화 처리하고, HashMap은 비동기화 처리한다.
 * 3 Hashtable과 HashMap의 다른 기능들은 거의 같다.
 * 
 */

public class Java187_HashMap {

	public static void main(String[] args) {
		HashMap<Integer, Number> map = new HashMap<Integer, Number>();
		map.put(1,  new Integer(10));
		map.put(2,  new Double(4.5));
		map.put(1,  new Float(2.3f));
		
		System.out.println(map.get(2));
		
	} //end main()

} //end class
