package java0418_basic;

public class Java008_operator {
	public static void main(String[] args) {
		int x=3;
		
		//증가연산자 : 변수의 값을 1증가시켜라.
		++x; //x=x+1 이런 의미 but, 내부에서 처리방식은 다르다. 앞뒤순서 상관없음
		System.out.println("x=" + x);
		
		int y=5;
		--y;
		System.out.println("y=" + y);
		
		
	}// end main()	
} // end class
